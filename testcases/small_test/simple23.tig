let
 function printint(i: int) =
  let function f(i:int) = if 0<i 
	     then (f(i/10); print(chr(i-i/10*10+ord("0"))))
   in if i<0 then (print("-"); f(-i))
      else (if 0<i then f(i)
      else print("0"))
  end

    type intArray = array of int
    type point = {x: int, y:int}

    var myarr := intArray [10] of 0
    var mypt := point {x=1, y=2}
in
    (
    myarr[2] := 3;
    printint(myarr[2]);
    printint(mypt.x)
    )
end
