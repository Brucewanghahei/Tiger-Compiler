let
  function readline():string =
    let
      var s := ""
      var c := getchar()
    in
      while not(c = "\n")
      do
        (
          print(c)
          ;s := s + c
          ;c := getchar()
        )
        ;
      s
    end
  function f():string = readline()
in
  print("readline: " + f() + "\n")
end
